{
    "kind": "rbd",
    "name": "Reliability Block Diagram",
    "nodes": {
        "node": {
            "numberOfIncomingConnections": -1,
            "numberOfOutgoingConnections": -1,
            "allowConnectionTo": [],

            "name": "Node",

            "propertyMenuEntries": {
                "name": {
                    "kind": "text",
                    "displayName": "Name"
                }
            },

            "propertyMirrors": {
                "name": {
                    "position": "bottom",
                    "style": ["bold", "large"]
                }
            },

            "connector": {
                "offset": {
                    "top":    0,
                    "bottom": 0
                }
            }
        },

        "start": {
            "inherits": "node",
            "name": "Start",
            "image": "start.svg",
            "numberOfIncomingConnections": 0,
            "allowConnectionTo": ["end", "block", "out_of"],
            "excludeFromShapesMenu": true,
            "deletable": false,

            "connector": {
                "offset": {
                    "right": -9.1
                }
            }
        },

        "end": {
            "inherits": "node",
            "name": "End",
            "image": "end.svg",
            "numberOfOutgoingConnections": 0,
            "excludeFromShapesMenu": true,
            "deletable": false,

            "connector": {
                "offset": {
                    "left":   8.1
                }
            }
        },

        "block": {
            "inherits": "node",
            "name": "Block",
            "excludeFromShapesMenu": false,
            "image": "block.svg",
            "allowConnectionTo": ["end", "block", "out_of"],

            "probability": 0,

            "propertyMenuEntries": {
                "probability": {
                    "kind": "number",
                    "displayName": "Probability",
                    "disabled": false,
                    "min": 0,
                    "max": 1,
                    "step": 0.01
                }
            },

            "propertyMirrors": {
                "probability": {
                    "position": "bottom",
                    "style": ["italic"],
                    "prefix": "p="
                }
            }
        },

        "out_of": {
            "inherits": "node",
            "name": "Out of",
            "excludeFromShapesMenu": false,
            "image": "out_of.svg",
            "allowConnectionTo": ["block", "end"],
            "numberOfOutgoingConnections": 1,

            "out_of": [1, 1],

            "propertyMenuEntries": {
                "out_of": {
                    "kind": "range",
                    "displayName": "Out of",
                    "min": 1,
                    "step": 1
                }
            },

            "propertyMirrors": {
                "out_of": {
                    "position": "bottom",
                    "style": ["italic"]
                }
            }
        }
    },

    "shapeMenuNodeDisplayOrder": [
        "block",
        "out_of",
        "start",
        "end"
    ],

    "propertiesDisplayOrder": [
        "name",
        "probability",
        "out_of"
    ],

    "defaults": {
        "nodes": [
            {
                "kind": "start",
                "x": 5,
                "y": 1
            },
            {
                "kind": "end",
                "x": 10,
                "y": 1
            }
        ]
    }
}
