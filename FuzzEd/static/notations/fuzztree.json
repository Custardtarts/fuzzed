{
    "kind": "fuzztree",
    "name": "Fuzz Tree",
    "nodes": {
        "node": {
            "numberOfOutgoingConnections": -1,
            "numberOfIncomingConnections":  1,

            "allowConnectionTo": ["node"],

            "deletable": true,

            "connector": {
                "offset": {
                    "top":    0,
                    "bottom": 0
                }
            },

            "properties": {
                "name": {
                    "kind": "text",
                    "displayName": "Name",
                    "default": "Node",

                    "mirror": {
                        "position": "bottom",
                        "style": ["bold", "large"]
                    }
                }
            }
        },

        "event": {
            "inherits": "node",

            "properties": {
                "name": {
                    "default": "Event"
                },
                "optional": {
                    "kind": "bool",
                    "displayName": "Optional",
                    "default": false
                }
            }
        },

        "gate": {
            "inherits": "node",

            "properties": {
                "name": {
                    "default": "Gate"
                }
            }
        },

        "variationPoint": {
            "inherits": "node",

            "properties": {
                "name": {
                    "default": "Variation Point"
                }
            }
        },

        "basicEvent": {
            "inherits": "event",
            "image": "basic_event.svg",
            "help": "Initiating failure in a basic component",

            "numberOfOutgoingConnections": 0,

            "properties": {
                "name": {
                    "default": "Basic Event"
                },

                "probability": {
                    "kind": "compound",
                    "displayName": "Probability",
                    "default": [0, [0.5, 0]],

                    "parts": [{
                        "kind": "epsilon",
                        "default": [0.5, 0],
                        "min": 0,
                        "max": 1,
                        "step": 0.01,
                        "epsilonStep": 0.01
                    }, {
                        "kind": "choice",
                        "default": [0.5, 0.3],
                        "choices": [
                            "never",
                            "very unlikely",
                            "unlikely",
                            "more or less",
                            "likely",
                            "very likely",
                            "always"
                        ],
                        "values": [
                            [0,    0],
                            [0.2,  0.1],
                            [0.33, 0.2],
                            [0.5,  0.3],
                            [0.66, 0.2],
                            [0.8,  0.1],
                            [1,    0]
                        ]
                    }],

                    "mirror": {
                        "position": "bottom",
                        "style": ["italic"],
                        "prefix": "p="
                    }
                },

                "cost": {
                    "kind": "numeric",
                    "displayName": "Cost",
                    "default": 1,
                    "min": 0,
                    "step": 0.01,

                    "mirror": {
                        "position": "bottom",
                        "prefix": "c="
                    }
                }
            }
        },

        "basicEventSet": {
            "inherits": "basicEvent",
            "image": "basic_event_set.svg",
            "help": "Set of basic events with identical properties",

            "properties": {
                "name": {
                    "default": "Basic Event Set"
                },

                "cardinality": {
                    "kind": "numeric",
                    "displayName": "Cardinality",
                    "default": 1,
                    "min": 1,
                    "step": 1,

                    "mirror": {
                        "position": "bottom",
                        "style": ["italic"],
                        "prefix": "#"
                    }
                }
            }
        },

        "intermediateEvent": {
            "inherits": "event",

            "image": "intermediate_event.svg",
            "help": "Failure resulting from a combination of previous events",

            "properties": {
                "name": {
                    "default": "Intermediate Event"
                }
            }
        },

        "intermediateEventSet": {
            "inherits": "intermediateEvent",

            "image": "intermediate_event_set.svg",
            "help": "Set of intermediate events",

            "properties": {
                "name": {
                    "default": "Intermediate Event Set"
                },

                "cardinality": {
                    "kind": "numeric",
                    "displayName": "Cardinality",
                    "default": 1,
                    "min": 1,
                    "step": 1,

                    "mirror": {
                        "position": "bottom",
                        "style": ["italic"],
                        "prefix": "#"
                    }
                }
            }
        },

        "andGate": {
            "inherits": "gate",

            "image": "and_gate.svg",
            "help": "Output event occurs if all input events occur",

            "properties": {
                "name": {
                    "default": "AND Gate"
                }
            }
        },

        "orGate": {
            "inherits": "gate",

            "image": "or_gate.svg",
            "help": "Output event occurs if one or more input events occur",

            "properties": {
                "name": {
                    "default": "OR Gate"
                }
            },

            "connector": {
                "offset": {
                    "bottom": -7
                }
            }
        },

        "xorGate": {
            "inherits": "gate",

            "image": "xor_gate.svg",
            "help": "Output event occurs if exactly one of the input events occur",

            "properties": {
                "name": {
                    "default": "XOR Gate"
                }
            }
        },

        "votingOrGate": {
            "inherits": "gate",

            "image": "voting_or_gate.svg",
            "help": "Output event occurs if the given number of input events occur",

            "properties": {
                "name": {
                    "default": "Voting OR Gate"
                },
                "k": {
                    "kind": "numeric",
                    "displayName": "k",
                    "min": 1,
                    "step": 1,
                    "default": 1,

                    "mirror": {
                        "position": "bottom",
                        "style": ["italic"],
                        "prefix": "k="
                    }
                }
            }
        },

        "undevelopedEvent": {
            "inherits": "event",

            "numberOfOutgoingConnections": 0,
            "image": "undeveloped_event.svg",
            "help": "Event with no information available or insignificant impact",

            "properties": {
                "name": {
                    "default": "Undeveloped Event"
                },
                "optional": {
                    "default": null
                }
            }
        },

        "featureVariation": {
            "inherits": "variationPoint",

            "allowConnectionTo": ["event", "variationPoint", "transferIn"],
            "image": "feature_variation.svg",
            "help": "Placeholder for one of the input events",

            "properties": {
                "name": {
                    "default": "Feature Variation"
                }
            },

            "connector": {
                "dashstyle": "4 2"
            },

            "changedChildProperties": {
                "optional": null
            }
        },

        "redundancyVariation": {
            "inherits": "variationPoint",

            "allowConnectionTo": ["intermediateEventSet", "basicEventSet"],
            "numberOfOutgoingConnections": 1,
            "image": "redundancy_variation.svg",
            "help": "Placeholder for a voting OR gate over a chosen number of the input events",

            "properties": {
                "name": {
                    "default": "Redundancy Variation"
                },
                "kFormula": {
                    "kind": "text",
                    "displayName": "K-Formula",
                    "default": "N-1",

                    "mirror": {
                        "position": "bottom",
                        "style": ["italic"],
                        "prefix": "k: "
                    }
                },
                "nRange": {
                    "kind": "range",
                    "displayName": "N-Range",
                    "min": 1,
                    "step": 1,
                    "default": [1, 2],

                    "mirror": {
                        "position": "bottom",
                        "style": ["italic"],
                        "prefix": "N: "
                    }
                }
            },

            "connector": {
                "dashstyle": "4 2"
            },

            "changedChildProperties": {
                "optional": null
            }
        },

        "houseEvent": {
            "inherits": "basicEvent",

            "numberOfOutgoingConnections": 0,
            "image": "house_event.svg",
            "help": "An event that is expected to occur and typically does not denote a failure",

            "properties": {
                "name": {
                    "default": "House Event"
                }
            }
        },

        "transferIn": {
            "inherits": "node",
            "image": "transfer_in.svg",
            "help": "Connects the output of a related FuzzTree, e.g. a subsystem, to this one",

            "numberOfIncomingConnections": 1,
            "numberOfOutgoingConnections": 0,

            "properties": {
                "name": {
                    "default": "Transfer In"
                },
                "transfer": {
                    "kind": "link",
                    "displayName": "Transfer",
                    "default": -1,

                    "mirror": {
                        "position": "bottom",
                        "style": ["bold", "large"],
                        "prefix": "◄ "
                    }
                },
                "transferMaxCost": {
                    "kind": "numeric",
                    "displayName": "Max Cost",
                    "min": 0,
                    "step": 1,
                    "default": 1,

                    "mirror": {
                        "position": "bottom",
                        "style": ["italic"],
                        "prefix": "max(c)="
                    }
                }
            }
        },

        "topEvent": {
            "inherits": "event",
            "image": "top_event.svg",
            "excludeFromShapesMenu": true,
            "numberOfIncomingConnections": 0,
            "numberOfOutgoingConnections": 1,
            "deletable": false,

            "properties": {
                "name": {
                    "default": "Top Event"
                },
                "optional": {
                    "default": null
                },
                "decompositions": {
                    "kind": "numeric",
                    "displayName": "Decompose",
                    "min": 1,
                    "step": 1,
                    "default": 1
                }
            }
        }
    },

    "propertiesDisplayOrder": [
        "name",
        "cost",
        "probability",
        "optional",
        "cardinality",
        "k",
        "kFormula",
        "nRange",
        "decompositions",
        "transfer",
        "transferMaxCost"
    ],

    "shapeMenuNodeDisplayOrder": [
        "basicEvent",
        "basicEventSet",
        "intermediateEvent",
        "intermediateEventSet",
        "andGate",
        "orGate",
        "xorGate",
        "votingOrGate",
        "featureVariation",
        "redundancyVariation",
        "houseEvent",
        "undevelopedEvent",
        "transferIn",
        "topEvent"
    ],

    "defaults": {
        "nodes": [
            {
                "kind": "topEvent",
                "x": 10,
                "y": 1
            }
        ]
    }
}
